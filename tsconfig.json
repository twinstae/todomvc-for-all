{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "react",
      "noEmit": true,
      "strict": true,
      "strictNullChecks": true,
      "noImplicitAny": true,
      "allowSyntheticDefaultImports": true,
    }
  }
  
