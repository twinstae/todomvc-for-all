{
	"compilerOptions": {
		"module": "ESNext",
		"moduleResolution": "node",
		"target": "ESNext",
		"types": ["vite/client"],
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"jsx": "preserve",
		"baseUrl": ".",
		"paths": {
			"@todomvc-core/*": ["todomvc-core/*"],
			"@todomvc-react/*": ["todomvc-react/src/*"],
			"@todomvc-svelte/*": ["todomvc-svelte/src/*"],
			"@todomvc-solid/*": ["todomvc-solid/src/*"],
			"@todomvc-vue/*": ["todomvc-vue/src/*"],
			"@todomvc-rescript/*": ["todomvc-rescript/src/*"],
			"@todomvc-elm/*": ["todomvc-elm/src/*"]
		}
	},
	"include": ["src", "test"]
}